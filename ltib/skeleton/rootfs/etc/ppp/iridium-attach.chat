# Check for errors
ABORT "NO CARRIER"
ABORT "NO DIALTONE"
ABORT "ERROR"
ABORT "NO ANSWER"
ABORT "BUSY"
ABORT "Username/Password Incorrect"
# Hang up the modem if in use
TIMEOUT 10
"" "AT+CBST=71,0,1"
# Log the signal strength measurement
"OK\r" "AT+CSQF"
"OK\r" "\c"
# Set the time out to receive a reply
TIMEOUT 150
# Dial the telephone number
"" "ATD$MSISDN"
"CONNECT" "\c"
