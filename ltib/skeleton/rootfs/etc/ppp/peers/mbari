file /etc/ppp/options.pptp

#for vpn.mbari.org
init "route del 134.89.18.12 2>/dev/null;:" #remove any preexisting host route to VPN server
pty  "exec pptp 134.89.18.12 --nolaunchpppd --logstring mbari --loglevel 0"
# --debug

linkname mbari
remotename mbari
ipparam mbari

noauth
defaultroute
usepeerdns

persist
holdoff 30
maxfail 0
