file /etc/ppp/options.pptp

#for vpn2.mbari.org
init "route del 134.89.19.10 2>/dev/null;:" #remove any preexisting host route to VPN server
pty  "exec pptp 134.89.19.10 --nolaunchpppd --idle-wait 180 --logstring mbari --loglevel 0"

#for vpn.mbari.org
#init "route del 134.89.18.10;:" #remove any preexisting host route to VPN server
#pty  "exec pptp 134.89.18.10 --nolaunchpppd --logstring mbari --loglevel 0"
# --debug

linkname mbari
remotename mbari
ipparam mbari
require-mppe-128

noauth
defaultroute
usepeerdns

holdoff 30
persist
maxfail 3
