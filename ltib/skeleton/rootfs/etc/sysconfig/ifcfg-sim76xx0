. $syscfg/ifcfg-wwan0
IFNAME=sim76xx0

ifPrep() {  #non-verizon -- must preprogram CGDCONT=1,"apn"
  atCmd 'AT+DIALMODE=0;+USBNETIP=1;+CFUN=1' t30
}

return #comment out this line for SIMCom7672G on the Verizon wireless network

ifPrep() {
  atCmd 'AT+DIALMODE=0;+USBNETIP=1' t8 &&
  atCmd 'AT+CGDCONT=1,"ip","vzwinternet";+CGDCONT=8,"ip","IMS";+CFUN=1' t30 &&
  sleep 15 &&
  atCmd 'AT+CGACT=1,8;+CGACT=0,1;+CGACT=1,1' t50
}
