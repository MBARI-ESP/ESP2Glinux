DEVICE=sl0
AUTOSTART=yes
BOOTPROTO=none

SLIPADDR=192.168.1.20     #master freewave
BROADCAST=192.168.1.31
NETMASK=255.255.255.240
MTU=1500

#start up SLIP for freewave master
NSprepCmd="/sbin/slattach -P /var/run/$DEVICE.pid -lLp slip \
-i \"IPADDR=$SLIPADDR; . /usr/share/ipinit.sh\" -s 115200 /dev/freewave &"
