proc /proc proc noauto	      #noauto as 1st option causes mountall to skip
none /dev devfs remount,rw    #for case of sysmount restart
 
/dev/root / auto remount,noatime #noauto as 1st option for r/o root
/dev/hda1 /cf auto noatime       #try to mount /cf at boot time

/cf/.SWAP swap swap #optional swapfile on compact flash disk


#END nfsmount service ignores entries below this line END#

tempest:/users /u nfs bg,intr,nosuid,nodev,noauto	#mbari
tempest:/tempbox /tempbox nfs bg,intr,nosuid,nodev,noauto	#mbari
tornado:/Engineering /engineering nfs bg,intr,nosuid,nodev,noauto #mbari
tornado:/vol/vol1/ProjectLibrary /project bg,nfs intr,nosuid,nodev,noauto #mbari

tornado:/ESP /ESP nfs bg,intr,nosuid,nodev,noauto #mbari

bufflehead:/etc /bufflehead/etc nfs intr,noauto
bufflehead:/home /bufflehead/home nfs intr,noauto
bufflehead:/var/log/ESPhost /var/log nfs intr,nolock,noauto

ESPbook:/home /book/home nfs intr,noauto
